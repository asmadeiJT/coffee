<div>
    <h2>Last 10 coffee cups</h2>
    <table class="table">
        <thead>
        <tr>
            <td>Coffee addict Name</td>
            <td>Cost</td>
            <td>Date</td>
            <td>Action</td>
        </tr>
        </thead>
        <tbody>
        {% for item in results %}
            <tr>
                <td>{{ item['name'] }}</td>
                <td>{{ item['cost']/100 }}</td>
                <td>{{ item['create_date']|date("m/d/Y h:i:s") }}</td>
                <td><a href="{{ path('delete_cup', {'id': item['id']}) }}" onclick="return confirm('are u sure?')">Delete</a></td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
</div>